etalert2\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialog: false,\r\n      fontList: [],\r\n      buyFontPrice: null,\r\n      buyFontId: null,\r\n      loginUser: {\r\n        userMileage: '',\r\n      },\r\n      myFontList: []\r\n    };\r\n  },\r\n  created () {\r\n    this.getAllFonts()\r\n    this.loginUser = this.$store.getters['getLoginUser'];\r\n  },\r\n  methods: {\r\n    getAllFonts: function () {\r\n      this.$store.dispatch(\"getAllFonts\")\r\n      .then((res) => {\r\n        this.fontList = res.data\r\n        console.log(this.fontList, '폰트리스트')\r\n        for (var i=0; i < this.fontList.length; i++) {\r\n          console.log(this.fontList[i].owned + '폰트리스트의 i')\r\n          if (this.fontList[i].owned) {\r\n            this.myFontList.push(this.fontList[i])\r\n          }\r\n        }\r\n        console.log(this.myFontList, '마이폰트리스트')\r\n        this.$store.commit('myFontList', this.myFontList)\r\n      })\r\n    },\r\n    sendInfo: function (price, id) {\r\n      console.log(price, '클릭한 폰트 가격')\r\n      console.log(id, '클릭한 폰트 아이디')\r\n      this.buyFontPrice = price\r\n      this.buyFontId = id\r\n    },\r\n    buyFont(fontId) {\r\n      console.log(fontId, '구매하려는 폰트 아이디')\r\n      this.$store.dispatch(\"buyFont\", fontId)\r\n      .then((res) => {\r\n        console.log(res)\r\n        this.loginUser.userMileage -= this.buyFontPrice\r\n        this.$store.commit('setLoginUser', this.loginUser)\r\n        this.$router.go()\r\n        Swal.fire({\r\n          icon: \"success\",\r\n          title:\r\n            '<span style=\"font-size:25px;\">성공적으로 구매되었습니다.</span>',\r\n          confirmButtonColor: \"#b0da9b\",\r\n          confirmButtonText: '<span style=\"font-size:18px;\">확인</span>',\r\n        });\r\n      })\r\n      .catch(() => {\r\n        Swal.fire({\r\n          icon: \"error\",\r\n          title:\r\n            '<span style=\"font-size:25px;\">마일리지가 부족합니다.</span>',\r\n          confirmButtonColor: \"#f27474\",\r\n          confirmButtonText: '<span style=\"font-size:18px;\">확인</span>',\r\n        });\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.Font_Item {\r\n  display: inline-block;\r\n  width: 286px;\r\n  height: 180px;\r\n  margin-bottom: 32px;\r\n  margin-left: 32px;\r\n  border-radius: 8px;\r\n  box-shadow: 3px 3px 11px rgba(166, 166, 168, 0.25);\r\n  overflow: hidden;\r\n}\r\n.Font_Header {\r\n  height: 50px;\r\n  border-bottom: 1px solid #e7e7e7;\r\n  line-height: 50px;\r\n}\r\n/* .Font_Content {\r\n  height: 129px;\r\n} */\r\n.font-name {\r\n  margin-left: 16px;\r\n  font-size: 16px;\r\n  font-weight: 500;\r\n}\r\n.Font_Price {\r\n  font-size: 16px;\r\n  float: right;\r\n  margin-right: 16px;\r\n  color: #ffb319;\r\n  font-weight: 600;\r\n}\r\n.Font_Discription {\r\n  /* width: 243px; */\r\n  height: 44px;\r\n  margin: 16px 16px 0 16px;\r\n  font-size: 15px;\r\n  color: #585858;\r\n  /* background-color: aquamarine; */\r\n}\r\n.Font_Btn {\r\n  width: 70px;\r\n  height: 32px;\r\n  margin-top: 21px;\r\n  margin-right: 16px;\r\n  float: right;\r\n  font-size: 15px;\r\n  /* color: #585858; */\r\n}\r\n#Dialog {\r\n  height: 180px;\r\n}\r\n#Dialog_Header {\r\n  height: 40px;\r\n}\r\n#Dialog_Close {\r\n  float: right;\r\n  margin-top: 5px;\r\n  margin-right: 5px;\r\n}\r\n#Dialog_Text {\r\n  width: 131px;\r\n  height: 38px;\r\n  margin: 0 auto;\r\n  margin-top: 5px;\r\n  font-size: 16px;\r\n  text-align: center;\r\n}\r\n#Dialog_Btn_Box {\r\n  width: 164px;\r\n  height: 32px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin: 0 auto;\r\n  margin-top: 30px;\r\n}\r\n.Dialog_Btn {\r\n  width: 76px;\r\n  height: 32px;\r\n  color: #585858;\r\n}\r\n</style>"],"sourceRoot":"src/components/store"}]}