ontainer\">\r\n            <div class=\"page\" id=\"page1\">\r\n                <div v-if=\"introMode\" id=\"Login_Outer\">\r\n                    <div id=\"Intro_Title\">\r\n                        PAPERS\r\n                    </div>\r\n                    <div id=\"Intro_content\">\r\n                        당신의 소중한 추억을 위한<br>\r\n                        일상 기록 서비스\r\n                    </div>\r\n                    <v-btn\r\n                    id=\"Spread_Btn\"\r\n                    @click=\"spread\"\r\n                    text\r\n                    >\r\n                    Click&nbsp;&nbsp; >\r\n                    </v-btn>\r\n                </div>\r\n                <div v-if=\"!introMode\" id=\"Login_left\">\r\n                    <div id=\"Left_Title\">\r\n                        PAPERS\r\n                    </div>\r\n                    <div id=\"Left_content\">\r\n                        당신의 소중한 추억을 위한<br>\r\n                        일상 기록 서비스\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"page\" id=\"page2\">\r\n                <div>\r\n                    <v-text-field \r\n                    class=\"Login_Input\" \r\n                    label=\"ID\"\r\n                    color=\"#FFB319\"\r\n                    :rules=\"rules\"></v-text-field>\r\n                </div>\r\n                <div>\r\n                    <v-text-field \r\n                    class=\"Login_Input\" \r\n                    type=\"password\" \r\n                    label=\"PW\"\r\n                    color=\"#FFB319\"\r\n                    :rules=\"rules\"></v-text-field>\r\n                </div>\r\n                <v-btn\r\n                @click=\"login\"\r\n                id=\"Login_Btn\"\r\n                >로그인</v-btn>\r\n                <v-btn\r\n                id=\"GoSignUp_Btn\"\r\n                @click=\"goSignUp\"\r\n                text\r\n                >\r\n                처음이신가요?&nbsp;&nbsp; >\r\n                </v-btn>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return{\r\n            introMode: true,\r\n            rules: [\r\n                value => !!value || '입력창을 확인해주세요.',\r\n            ]\r\n        }\r\n    },\r\n    methods:{\r\n        login() {\r\n            this.$router.push('main')\r\n        },\r\n        goSignUp() {\r\n            this.$router.push('signUp')\r\n        },\r\n        spread() {\r\n            setTimeout(() => this.introMode = false, 250);\r\n            const page1 = document.getElementById('page1')\r\n            const container = document.getElementById('Login_Container')\r\n            page1.style.transform = \"rotateY(-180deg)\"\r\n            page1.style.transition = \"1s\"\r\n            container.style.transform = \"translate(200px,0px)\"\r\n            container.style.transition = \"1s\"\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#Intro_Container{\r\n    display:flex;   \r\n    border-right:1px solid lightgrey;\r\n    border-left:1px solid lightgrey;\r\n    margin:0 auto;\r\n    width:1280px;\r\n    height:100vh;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n#Login_Container {\r\n    position: relative;\r\n    width: 400px;\r\n    height: 580px;\r\n    perspective: 1000px;\r\n}\r\n#Login_Outer{\r\n    width:400px;\r\n    height:580px;\r\n    margin-left:-33px;\r\n    background:#FFB319;\r\n    display:flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n}\r\n#Intro_Title{\r\n    width:190px;\r\n    height:70px;\r\n    line-height:70px;\r\n    text-align: center;\r\n    font-size:60px;\r\n    color:white;\r\n    font-family: Cafe24Syongsyong;\r\n}\r\n#Intro_content{\r\n    font-size:20px;\r\n    text-align: center;\r\n    color:white;\r\n    margin-top:150px;\r\n}\r\n#Spread_Btn{\r\n    color:white;\r\n    margin-top:30px;\r\n    font-size:18px;\r\n}\r\n#Login_left{\r\n    width:400px;\r\n    height:580px;\r\n    display:flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    flex-direction: column;\r\n    transform: scaleX(-1);\r\n    margin-left:-33px;\r\n}\r\n#Left_Title{\r\n    width:190px;\r\n    height:70px;\r\n    line-height:70px;\r\n    text-align: center;\r\n    font-size:60px;\r\n    color:black;\r\n    font-family: Cafe24Syongsyong;\r\n}\r\n#Left_content{\r\n    font-size:20px;\r\n    text-align: center;\r\n    color:black;\r\n    margin-top:150px;\r\n}\r\n#Login_Btn{\r\n    width:200px;\r\n    background: #FFB319;\r\n    color:white;\r\n    position:relative;\r\n    font-size:20px;\r\n    top:30px;\r\n}\r\n#GoSignUp_Btn{\r\n    position:relative;\r\n    top:50px;\r\n    font-size:18px;\r\n    color:black;\r\n}\r\n.Login_Input{\r\n    width:200px;\r\n    font-size:25px;\r\n}\r\n\r\n/* page animation */\r\n.page {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  padding: 0 32px;\r\n  background-color: #ffffff;\r\n  box-shadow: 0 0 1px lightgrey, -1px 0 1px lightgrey;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n#page1 {\r\n  z-index: 1;\r\n  transform-origin: left center;\r\n  transition-duration: 1s;\r\n  transition-timing-function: ease-in;\r\n  border:1px solid lightgrey;\r\n}\r\n#page2 {\r\n  z-index: 0;\r\n  display:flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  border:1px solid lightgrey;\r\n}\r\n</style>"]}]}