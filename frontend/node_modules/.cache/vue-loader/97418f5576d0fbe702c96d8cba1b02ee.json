CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Login.vue","sourceRoot":"src/views/intro","sourcesContent":["<template>\r\n  <div id=\"Intro_Container\">\r\n    <div id=\"Login_Container\">\r\n      <div id=\"Login_Box\"></div>\r\n      <div class=\"page1\" id=\"page1\">\r\n        <div v-if=\"introMode\" id=\"Login_Outer\">\r\n          <img class=\"Img\" src=\"../../assets/image/paper.png\" />\r\n          <div id=\"Intro_Title\">PAPERS</div>\r\n          <div id=\"Intro_content\">\r\n            당신의 소중한 추억을 위한<br />\r\n            일상 기록 서비스\r\n          </div>\r\n          <v-btn id=\"Spread_Btn\" @click=\"spread\" text>\r\n            Click&nbsp;&nbsp; >\r\n          </v-btn>\r\n\r\n          <!-- <br>    \r\n          <v-btn\r\n            id=\"connection\"\r\n            @click=\"connection\"\r\n          >\r\n          연결\r\n          </v-btn>\r\n          <br>    \r\n          <v-btn\r\n            id=\"publish\"\r\n            @click=\"publish\"\r\n          >\r\n          알림 발생\r\n          </v-btn>\r\n          <br>    \r\n          <v-btn\r\n            id=\"close\"\r\n            @click=\"close\"\r\n          >\r\n          연결 끊기\r\n          </v-btn> -->\r\n\r\n        </div>\r\n        <div v-if=\"!introMode\" id=\"Login_left\">\r\n          <div id=\"Login_Left_In\">\r\n            <div id=\"Line\"></div>\r\n            <div class=\"Img2\">\r\n              <img style=\"width: 87px\" src=\"../../assets/image/paper.png\" />\r\n            </div>\r\n            <div id=\"Left_Title\">PAPERS</div>\r\n            <div id=\"Left_content\">\r\n              당신의 소중한 추억을 위한<br />\r\n              일상 기록 서비스\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"page2\" id=\"page2\">\r\n        <div id=\"Login_Right_In\">\r\n          <div id=\"Form_Box\">\r\n            <div>\r\n              <v-text-field\r\n                v-on:keyup.enter=\"login\"\r\n                class=\"Login_Input\"\r\n                label=\"ID\"\r\n                color=\"#FFB319\"\r\n                v-model=\"userId\"\r\n                :rules=\"rules\"\r\n              ></v-text-field>\r\n            </div>\r\n            <div>\r\n              <v-text-field\r\n                v-on:keyup.enter=\"login\"\r\n                class=\"Login_Input\"\r\n                type=\"password\"\r\n                label=\"PW\"\r\n                color=\"#FFB319\"\r\n                v-model=\"userPwd\"\r\n                :rules=\"rules\"\r\n              ></v-text-field>\r\n            </div>\r\n            <v-btn @click=\"login\" id=\"Login_Btn\">로그인</v-btn>\r\n            <v-btn id=\"GoSignUp_Btn\" @click=\"goSignUp\" text>\r\n              처음이신가요?&nbsp;&nbsp; >\r\n            </v-btn>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\nimport { mapGetters } from 'vuex'\r\nexport default {\r\n  data() {\r\n    return {\r\n      API_NOTIFICATION_URL: 'https://localhost/api',\r\n      alarmEventSource: null,\r\n      introMode: true,\r\n      userId: \"\",\r\n      userPwd: \"\",\r\n      rules: [(value) => !!value || \"입력창을 확인해주세요.\"],\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getAlarmEventSource\"]),\r\n  },\r\n  methods: {\r\n    login() {\r\n      const user = {\r\n        userId: this.userId,\r\n        userPwd: this.userPwd,\r\n      };\r\n      this.$store\r\n        .dispatch(\"login\", user)\r\n        .then((result) => {\r\n          const loginUser = {\r\n            userId: result.data.userId,\r\n            userNickname: result.data.userNickname,\r\n            userMileage: result.data.userMileage,\r\n            userProfile: result.data.userProfile,\r\n            userToken: result.data.userToken,\r\n          };\r\n          // Store에 loginUser 정보 저장\r\n          this.$store.commit(\"setLoginUser\", loginUser);\r\n\r\n\r\n          const alarmEventSource = new EventSource(this.API_NOTIFICATION_URL + `/notification/subscribe?uuid=${loginUser.userToken}`);\r\n          // 알림 발생 시 이벤트 처리\r\n          alarmEventSource.onmessage = (e) => {\r\n            alert(e.data)\r\n          }\r\n          this.$store.commit('setAlarmEventSource', alarmEventSource)\r\n\r\n          this.$router.push(\"main\");\r\n        })\r\n        .catch(() => {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title:\r\n              '<span style=\"font-size:25px;\">아이디 또는 비밀번호를 확인해주세요</span>',\r\n            confirmButtonColor: \"#f27474\",\r\n            confirmButtonText: '<span style=\"font-size:18px;\">확인</span>',\r\n          });\r\n        });\r\n    },\r\n    goSignUp() {\r\n      this.$router.push(\"signUp\");\r\n    },\r\n    spread() {\r\n      setTimeout(() => (this.introMode = false), 250);\r\n      const page1 = document.getElementById(\"page1\");\r\n      const container = document.getElementById(\"Login_Container\");\r\n      const loginBox = document.getElementById(\"Login_Box\");\r\n      page1.style.transform = \"rotateY(-180deg)\";\r\n      page1.style.transition = \"1s\";\r\n      container.style.transform = \"translate(230px,0px)\";\r\n      container.style.transition = \"1s\";\r\n      loginBox.style.boxShadow = \"2px 3px 12px 2px rgba(35, 35, 35, 0.25)\";\r\n      loginBox.style.transition = \"1.2s\";\r\n    },\r\n    connection() {\r\n      // alert('tes')\r\n      const API_NOTIFICATION_URL = 'https://localhost/api'\r\n      // let url = API_NOTIFICATION_URL + \"/notification/push?uuid=dsasasa\";\r\n      // let eventSource = new EventSource(url, {withCredentials: true});\r\n      // eventSource.onmessage = function (e) {\r\n      //   alert(e.data)\r\n      // }\r\n      // console.log(eventSource)\r\n\r\n      const alarmEventSource = new EventSource(API_NOTIFICATION_URL + `/notification/subscribe?uuid=${Math.random()}`);\r\n      this.alarmEventSource = alarmEventSource\r\n      alarmEventSource.onopen = (e) => {\r\n        console.log('---open---')\r\n        console.log(e)\r\n        console.log('----------')\r\n      }\r\n      alarmEventSource.onerror = (e) => {\r\n        console.log('---error---')\r\n        console.log(e)\r\n        console.log('----------')\r\n      }\r\n      alarmEventSource.onmessage = (e) => {\r\n        alert(e.data);\r\n      }\r\n    },\r\n    close () {\r\n      this.getAlarmEventSource.close()\r\n    },\r\n    publish() {\r\n      const API_NOTIFICATION_URL = 'https://localhost/api'\r\n      fetch(API_NOTIFICATION_URL + `/notification/publish?message=알림 발생입니다.`);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#Intro_Container {\r\n  display: flex;\r\n  border-right: 1px solid lightgrey;\r\n  border-left: 1px solid lightgrey;\r\n  margin: 0 auto;\r\n  width: 1270px;\r\n  height: 100vh;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n#Login_Container {\r\n  position: relative;\r\n  width: 461px;\r\n  height: 644px;\r\n  perspective: 1500px;\r\n}\r\n#Login_Box {\r\n  width: 920px;\r\n  height: 644px;\r\n  border-radius: 20px;\r\n  position: relative;\r\n  left: -460px;\r\n}\r\n#Login_Outer {\r\n  width: 461px;\r\n  height: 644px;\r\n  margin-left: -33px;\r\n  background: #fae7cb;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  border-top-right-radius: 20px;\r\n  border-bottom-right-radius: 20px;\r\n  box-shadow: 2px 3px 12px 2px rgba(35, 35, 35, 0.25);\r\n}\r\n#Intro_Title {\r\n  width: 190px;\r\n  height: 70px;\r\n  line-height: 70px;\r\n  text-align: center;\r\n  font-size: 62px;\r\n  color: #ffb319;\r\n  font-family: Cafe24Syongsyong;\r\n}\r\n#Intro_content {\r\n  font-size: 20px;\r\n  text-align: center;\r\n  color: #585858;\r\n  margin-top: 75px;\r\n}\r\n#Spread_Btn {\r\n  color: #222222;\r\n  margin-top: 30px;\r\n  font-size: 18px;\r\n}\r\n#Login_left {\r\n  width: 461px;\r\n  height: 644px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  transform: scaleX(-1);\r\n  margin-left: -33px;\r\n  background: #fae7cb;\r\n  position: relative;\r\n  border-top-left-radius: 20px;\r\n  border-bottom-left-radius: 20px;\r\n}\r\n#Left_Title {\r\n  width: 190px;\r\n  height: 70px;\r\n  line-height: 70px;\r\n  text-align: center;\r\n  font-size: 60px;\r\n  color: #ffb319;\r\n  font-family: Cafe24Syongsyong;\r\n  position: relative;\r\n  left: -1.5px;\r\n  top: -1px;\r\n}\r\n#Left_content {\r\n  font-size: 20px;\r\n  text-align: center;\r\n  color: #222222;\r\n  margin-top: 80px;\r\n  position: relative;\r\n  left: -1px;\r\n  top: -1px;\r\n}\r\n#Login_Btn {\r\n  width: 220px;\r\n  background: #ffb319;\r\n  color: white;\r\n  font-size: 20px;\r\n  position: relative;\r\n  top: -10px;\r\n}\r\n#GoSignUp_Btn {\r\n  width: 200px;\r\n  font-size: 18px;\r\n  position: relative;\r\n  top: 35px;\r\n  color: #222222;\r\n}\r\n.Login_Input {\r\n  width: 220px;\r\n  font-size: 25px;\r\n  margin-top: -20px;\r\n}\r\n\r\n/* page animation */\r\n.page1 {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  padding: 0 32px;\r\n  background-color: #fae7cb;\r\n  width: 99%;\r\n  height: 100%;\r\n  border-top-right-radius: 20px;\r\n  border-bottom-right-radius: 20px;\r\n}\r\n.page2 {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  padding: 0 32px;\r\n  background-color: #fae7cb;\r\n  border-top-right-radius: 20px;\r\n  border-bottom-right-radius: 20px;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n#page1 {\r\n  z-index: 1;\r\n  transform-origin: left center;\r\n  transition-duration: 1s;\r\n  transition-timing-function: ease-in;\r\n}\r\n#page2 {\r\n  z-index: 0;\r\n  display: flex;\r\n  justify-content: center;\r\n  /* align-items: flex-end; */\r\n  flex-direction: column;\r\n}\r\n#Line {\r\n  height: 590px;\r\n  border: 1px solid #d7d7d7;\r\n  position: absolute;\r\n  top: 10px;\r\n  left: 443px;\r\n}\r\n#Login_Left_In {\r\n  margin-left: auto;\r\n  width: 445px;\r\n  height: 610px;\r\n  background: white;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  position: relative;\r\n  margin-left: 18px;\r\n}\r\n#Login_Right_In {\r\n  position: relative;\r\n  left: -31px;\r\n  width: 443px;\r\n  height: 610px;\r\n  background: white;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n#Form_Box {\r\n  width: 220px;\r\n  height: 272px;\r\n  /* border:1px solid red; */\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  align-items: flex-start;\r\n}\r\n.Img {\r\n  width: 87px;\r\n  height: 87px;\r\n}\r\n.Img2 {\r\n  width: 87px;\r\n  height: 87px;\r\n  position: relative;\r\n  left: -1px;\r\n  top: -1px;\r\n}\r\n</style>"]}]}