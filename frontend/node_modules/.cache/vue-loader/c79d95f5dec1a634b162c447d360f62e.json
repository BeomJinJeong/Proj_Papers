eft_content\">\r\n        당신의 소중한 추억을 위한<br />\r\n        일상 기록 서비스\r\n      </div>\r\n    </div>\r\n    <div id=\"SignUp_right\">\r\n      <div id=\"Form_Box\">\r\n        <div>\r\n          <v-text-field\r\n            class=\"SignUp_Input\"\r\n            label=\"아이디\"\r\n            color=\"#FFB319\"\r\n            v-model=\"userId\"\r\n            :rules=\"rules\"\r\n          ></v-text-field>\r\n        </div>\r\n        <div>\r\n          <v-text-field\r\n            class=\"SignUp_Input\"\r\n            type=\"password\"\r\n            label=\"비밀번호\"\r\n            color=\"#FFB319\"\r\n            v-model=\"userPwd\"\r\n            :rules=\"rules\"\r\n          ></v-text-field>\r\n        </div>\r\n        <div>\r\n          <v-text-field\r\n            class=\"SignUp_Input\"\r\n            type=\"password\"\r\n            label=\"비밀번호확인\"\r\n            color=\"#FFB319\"\r\n            v-model=\"userPwdCheck\"\r\n            :rules=\"rules\"\r\n          ></v-text-field>\r\n        </div>\r\n        <div>\r\n          <v-text-field\r\n            class=\"SignUp_Input\"\r\n            label=\"닉네임\"\r\n            color=\"#FFB319\"\r\n            v-model=\"userNickname\"\r\n            :rules=\"rules\"\r\n          ></v-text-field>\r\n        </div>\r\n        <v-btn @click=\"signUp\" id=\"SignUp_Btn\">회원가입</v-btn>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\nimport { mapActions } from \"vuex\";\r\nconst Login = \"Login\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      rules: [(value) => !!value || \"입력창을 확인해주세요.\"],\r\n      userId: \"\",\r\n      userPwd: \"\",\r\n      userPwdCheck: \"\",\r\n      userNickname: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions(Login, [\"sign\"]),\r\n    signUp() {\r\n      const user = {\r\n        userId: this.userId,\r\n        userPwd: this.userPwd,\r\n        userNickname: this.userNickname,\r\n        // userName: \"이름\",\r\n      };\r\n      if(user.userId.length < 4 || user.userPwd.length < 4) {\r\n        Swal.fire({\r\n            icon: \"error\",\r\n            title: '<span style=\"font-size:25px;\">아이디는 4자 이상, 비밀번호는 4자 이상 유효합니다.</span>',\r\n            confirmButtonColor: \"#f27474\",\r\n            confirmButtonText: '<span style=\"font-size:18px;\">확인</span>',\r\n          });\r\n          \r\n      } else {\r\n        this.$store\r\n        .dispatch(\"signUp\", user)\r\n        .then(() => {\r\n          Swal.fire({\r\n            icon: \"success\",\r\n            title:\r\n              '<span style=\"font-size:25px;\">회원가입을 축하드립니다.</span>',\r\n            confirmButtonColor: \"#b0da9b\",\r\n            confirmButtonText: '<span style=\"font-size:18px;\">확인</span>',\r\n          });\r\n          this.$router.push(\"/\").catch(() => {});\r\n        })\r\n        .catch(() => {\r\n          Swal.fire({\r\n            icon: \"error\",\r\n            title: '<span style=\"font-size:25px;\">중복된 아이디입니다.</span>',\r\n            confirmButtonColor: \"#f27474\",\r\n            confirmButtonText: '<span style=\"font-size:18px;\">확인</span>',\r\n          });\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#SignUp_Container {\r\n  background: #fae7cb;\r\n  border-radius: 20px;\r\n  width: 922px;\r\n  height: 644px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  box-shadow: 2px 3px 12px 2px rgba(35, 35, 35, 0.25);\r\n}\r\n#SignUp_left {\r\n  width: 443px;\r\n  height: 610px;\r\n  background: white;\r\n  float: left;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  position: relative;\r\n}\r\n#Left_Title {\r\n  width: 190px;\r\n  height: 70px;\r\n  line-height: 70px;\r\n  text-align: center;\r\n  font-size: 60px;\r\n  color: #ffb319;\r\n  font-family: Cafe24Syongsyong;\r\n}\r\n#Left_content {\r\n  font-size: 20px;\r\n  text-align: center;\r\n  color: #222